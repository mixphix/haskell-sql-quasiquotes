{
	"name": "haskell-sql-quasiquotes",
	"displayName": "Haskell SQL QuasiQuotes",
	"description": "Syntax highlighting for SQL quasiquotes within Haskell files.",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.55.0"
	},
	"keywords": [
		"language",
		"haskell",
		"SQL",
		"highlight",
		"syntax"
	],
	"license": "BSD-3-Clause",
	"categories": [
		"Programming Languages"
	],
	"extensionDependencies": [
		"justusadam.language-haskell"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/bi-functor/haskell-sql-quasiquotes.git"
	},
	"publisher": "mel-brown",
	"contributes": {
		"grammars": [
			{
				"injectTo": [
					"source.haskell"
				],
				"scopeName": "inline.sql",
				"path": "./syntaxes/sql.tmLanguage.json",
				"embeddedLanguages": {
					"meta.embedded.block.sql": "sql"
				}
			}
		]
	}
}
